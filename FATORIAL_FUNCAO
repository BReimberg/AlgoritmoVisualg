Algoritmo "fatorial_funcao"
//fatorial de um número

Var
  n, f: inteiro

Funcao Fatorial(v: inteiro): inteiro
var
  c, r: inteiro
inicio
      r <- 1
      Para c <- 1 ate v faca
         r <- r * c
      FimPara
      Retorne r
FimFuncao

Inicio
   escreva("Digite um número: ")
   leia(n)
   f <- Fatorial(n)
   escreval("O valor de ", n, "! e igual a ", f)

Fimalgoritmo
