Algoritmo "preencher_matriz"
//Preencher uma matriz 3x2 com valores via teclado

Var
   mat: vetor[1..3, 1..2] de inteiro
   l, c: inteiro

Inicio
     Para l <- 1 ate 3 faca
          Para c <- 1 ate 2 faca
               escreva("Digite o valor da posição[", l, ",", c, "]: ")
               leia(mat[l,c])
          FimPara
     FimPara
     
     Para l <- 1 ate 3 faca
          Para c <- 1 ate 2 faca
          escreva(mat[l,c]:4)
          FimPara
          escreval()
     FimPara

Fimalgoritmo
