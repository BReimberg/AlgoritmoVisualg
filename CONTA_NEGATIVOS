Algoritmo "conta_negativos"

Var
  n, c, tn: inteiro

Inicio
   Repita
         escreval("Digite um n√∫mero: ")
         leia(n)
         Se (n < 0) entao
            tn <- tn + 1
         FimSe
         c <- c + 1
   Ate(c > 5)
   escreval("Foram digitados ", tn, " valores negativos.")

Fimalgoritmo
