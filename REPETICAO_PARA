Algoritmo "repeticao_para"

Var
   c: inteiro

Inicio
     Para c <- 10 ate 1 passo -1 faca
          escreval(c)
     FimPara

//contagem de 1 a 10
     Para c <- 1 ate 10 faca
          escreval(c)
     FimPara

//contagem de dois em dois
     Para c <- 1 ate 10 passo 2 faca
          escreval(c)
     FimPara

Fimalgoritmo
